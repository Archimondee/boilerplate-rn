{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "run-dev:android:windows": "SET ENVFILE=.env.development && react-native run-android --deviceId=emulator-5554",
    "run-dev:android:mac": "ENVFILE=.env.development react-native run-android --deviceId=emulator-5554",
    "run-dev:ios:mac": "ENVFILE=.env.development react-native run-ios",
    "run-staging:ios:mac": "ENVFILE=.env.staging react-native run-ios",
    "run-production:ios:mac": "ENVFILE=.env.production react-native run-ios",
    "run-prod:android:windows": "SET ENVFILE=.env.production && react-native run-android",
    "run-prod:android:mac": "ENVFILE=.env.production && react-native run-android",
    "build-dev:android:windows": "cd android && SET ENVFILE=.env.development && ./gradlew assembleRelease && cd ..",
    "build-dev:android:mac": "cd android && ENVFILE=.env.development && ./gradlew assembleRelease && cd ..",
    "build-prod:android:windows": "cd android && SET ENVFILE=.env.production && ./gradlew assembleRelease && cd ..",
    "build-prod:android:mac": "cd android && ENVFILE=.env.production && ./gradlew assembleRelease && cd ..",
    "clean": "cd android && ./gradlew clean && cd ..",
    "logcat": "adb -s emulator-5554 logcat | grep \"NODEJS-MOBILE\\|NODEJS-RN\\|AndroidRuntime\\|ReactNative\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.5",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/core": "^6.2.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "moment": "^2.29.3",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "redux-tools": "^3.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-react-native-wcandillon": "^3.8.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
